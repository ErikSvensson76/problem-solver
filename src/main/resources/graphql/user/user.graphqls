type UserResponse {
    user: User
    token: String
}

type UserActivationResponse {
    isActive: Boolean!
}

type User{
    id: ID!
    username: String!
    registrationDate: DateTime!
    userDetails: UserDetails
    roles: [String!]
    problems: [Problem!]
}

type UserDetails{
    id: ID!
    email: String!
    profile: Url
    displayName: String
}

type Role{
    id: ID!
    userRole: UserRole!
}

enum UserRole{
    APP_USER,APP_ADMIN
}